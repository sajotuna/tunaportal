<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.fourth.tuna.legacy.usertest.mapper.UserTestMapper">

<select id="findAll" resultType="co.fourth.tuna.legacy.usertest.vo.UserTestVO">
	SELECT *
	  FROM usertest 
</select>

<select id="findById" parameterType="co.fourth.tuna.legacy.usertest.vo.UserTestVO" resultType="co.fourth.tuna.legacy.usertest.vo.UserTestVO">
	select student_id, student_pwd, student_email, user_role 
	FROM usertest 
	WHERE student_id = #{studentId}
</select>



</mapper>