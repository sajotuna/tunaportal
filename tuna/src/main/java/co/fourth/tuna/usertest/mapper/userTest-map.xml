<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.fourth.tuna.usertest.mapper.userTest-map">

<select id="findAll" resultType="userTestVO">
	SELECT *
	  FROM usertest 
</select>

<select id="findById" parameterType="userTestVO" resultType="userTestVO">
	select student_id, student_pwd, student_email, user_role 
	FROM usertest 
	WHERE student_id = #{student_id}
</select>



</mapper>